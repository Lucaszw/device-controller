{
  "name": "@microdrop/device-controller",
  "version": "0.4.12",
  "description": "Controller DMF device via controlboard",
  "main": "build/device-controller.common.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-mode": "npm uninstall @microdrop/async && npm i ../microdrop-async",
    "prod-mode": "npm uninstall @microdrop/async && npm i @microdrop/async",
    "build": "webpack",
    "build-dev": "npm run build-microdrop-async && npm run build",
    "build-microdrop-async": "npm run --prefix ../microdrop-async build-dev",
    "update-deps": "ncu -ua @microdrop/async && npm install",
    "git-add": "git add package-lock.json && git add -p . && git commit",
    "publish-pkg": "npm run prod-mode && npm run build && npm version patch && npm publish && git push origin master"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "dependencies": {
    "@microdrop/async": "file:../microdrop-async",
    "backbone": "^1.3.3",
    "jquery": "^3.2.1",
    "keyboard-shortcut": "^1.2.0",
    "lodash": "^4.17.4",
    "mqtt": "^2.14.0",
    "three": "0.87.1",
    "three-helpers.mouse-event-handler": "^1.0.1",
    "three-helpers.svg-paths-group": "^1.0.3",
    "three-orbit-controls": "^82.1.0",
    "three-window-resize": "^1.0.4",
    "three.meshline": "^1.0.3",
    "three.planetransform": "^1.0.0",
    "threex-domevents": "^1.0.0",
    "two.js": "github:jonobr1/two.js#dev"
  },
  "devDependencies": {
    "express": "^4.16.2",
    "npm-link-recursive": "0.0.1"
  }
}
